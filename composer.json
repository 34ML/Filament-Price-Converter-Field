{
    "name": "34ml/filament-price-coverter-field",
    "description": "A price converter field for filament admin panel",
    "keywords": [
        "34ML",
        "laravel",
        "filament-price-coverter-field"
    ],
    "homepage": "https://github.com/34ml/filament-price-coverter-field",
    "license": "MIT",
    "authors": [
        {
            "name": "Mostafa Hassan",
            "email": "mostafa@34ml.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "_34ML\\FilamentPriceCoverterField\\": "src/",
            "_34ML\\FilamentPriceCoverterField\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "_34ML\\FilamentPriceCoverterField\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "_34ML\\FilamentPriceCoverterField\\FilamentPriceCoverterFieldServiceProvider"
            ],
            "aliases": {
                "FilamentPriceCoverterField": "_34ML\\FilamentPriceCoverterField\\Facades\\FilamentPriceCoverterField"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}